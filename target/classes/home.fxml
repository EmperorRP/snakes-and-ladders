<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="homePane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ap.final_project.Controller">
   <children>
      <ImageView fitHeight="309.0" fitWidth="542.0" layoutX="29.0" layoutY="-25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo_with_text-removebg-preview.png" />
         </image>
      </ImageView>
      <Button fx:id="gPLAY" layoutX="244.0" layoutY="219.0" mnemonicParsing="false" onAction="#homeToGame" prefHeight="53.0" prefWidth="112.0" style="-fx-background-color: #409c58;" text="PLAY" textFill="WHITE">
         <font>
            <Font size="30.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="gEXIT" layoutX="244.0" layoutY="298.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="53.0" prefWidth="112.0" style="-fx-background-color: #d66363;" text="EXIT" textFill="WHITE">
         <font>
            <Font size="30.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
